{
  "name": "umi-plugin-mpa-pug",
  "version": "0.1.2",
  "description": "MPA(multiple-page application) plugin for umi.",
  "repository": "wealthworks-fe/umi-plugin-mpa-pug",
  "main": "dist/index.js",
  "author": {
    "name": "zhongxia",
    "email": "izhongxia@sina.com"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "umi",
    "umi-plugin",
    "mpa"
  ],
  "scripts": {
    "build": "father-build --cjs rollup --target node",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish",
    "test": "umi-test"
  },
  "dependencies": {
    "ajv": "^6.9.1",
    "deasync-promise": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.2.1",
    "lodash": "^4.17.11",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "semver": "^5.6.0",
    "umi-plugin-types": "^0.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.3",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.18",
    "coveralls": "^3.0.2",
    "execa": "^1.0.0",
    "father-build": "^1.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "np": "4",
    "prettier": "^1.15.3",
    "react": "15.*",
    "react-dom": "15.*",
    "test-build-result": "^1.0.1",
    "umi": "2",
    "umi-test": "1",
    "umi-types": "^0.3.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "templates",
    "src",
    "dist"
  ],
  "license": "MIT"
}
